#include <iostream>
using namespace std;

class Samochody
{
    public:

    string marka;
    string model;
    int rocznik;
    int przebieg;

    void wczytaj()
    {
        cout<<"Prosze dodac samochod do bazy"<<endl;
        cout<<"Podaj marke samochodu: ";
        cin>>marka;
        cout<<"Podaj model samochodu: ";
        cin>>model;
        cout<<"Podaj rocznik samochodu: ";
        cin>>rocznik;
        cout<<"Podaj przebieg samochodu: ";
        cin>>przebieg;
    }

    void wypisz()
    {
        cout<<"Dodano samochod do bazy: "<<endl;
        cout<<"Marka: "<<marka<<" "<<"Model: "<<model<<" "<<"Rocznik: "<<rocznik<<" "<<"Przebieg: "<<przebieg<<endl;
    }
};

int main()
{
    Samochody s1, s2;
    s1.wczytaj();
    s1.wypisz();
    s2.wczytaj();
    s2.wypisz();
    return 0;
}
