#include <iostream>
//Zazwyczaj damskie imiona koncza sie na litere 'a'

using namespace std;

int main()
{
    string imie;

    cout<<"Podaj mi swoje imie, a zgadne Twoja plec!";
    cin>>imie;

    int dlugosc = imie.length();

    if(imie[dlugosc-1] == 'a')  cout<<"Wydaje mi sie, ze jest KOBIETA"<<endl;
    else                        cout<<"Wydaje mi sie, ze jestes MEZCZYZNA"<<endl;

    return 0;

}
